platform :ios, '9.0'

target 'Kanaco' do
    
    # Keys
    plugin 'cocoapods-keys', {
        :project => "Kanaco",
        :keys => [
        "APIKey"
        ]
    }
    
    # Uncomment the next line if you're using Swift or would like to use dynamic frameworks
    # use_frameworks!
    
    # Pods for Kanaco
    pod 'AFNetworking'
    
    target 'KanacoTests' do
        inherit! :search_paths
        # Pods for testing
    end
    
    target 'KanacoUITests' do
        inherit! :search_paths
        # Pods for testing
    end
end
